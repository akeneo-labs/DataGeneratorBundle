{
    "name": "akeneo-labs/data-generator-bundle",
    "type": "symfony-bundle",
    "description": "Akeneo PIM Bundle to generate data in order to test high volume operations",
    "keywords": ["pim", "akeneo", "ecommerce", "e-commerce", "data", "performances", "csv"],
    "homepage": "http://akeneo.com",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Benoit Jacquemont",
            "email": "benoit@akeneo.com",
            "homepage": "http://www.akeneo.com/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.4",
        "akeneo/pim-community-dev": "~1.4.0",
        "bheller/images-generator": "~1.0.0",
        "box/spout": "^2.4",
        "fzaninotto/faker": "1.4.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {"Pim\\Bundle\\DataGeneratorBundle\\": ""}
    },
    "target-dir": "Pim/Bundle/DataGeneratorBundle"
}
